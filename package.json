{
  "name": "coding_challenge",
  "version": "1.0.0",
  "description": "## Requirements - React  - Any state management library. Redux is preferred, but no penalty for using other state management libraries. No props drilling - MongoDB or Firestore database - Node.JS - Express.JS",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "setup-production":"npm run install-client && npm run build-client && npm install",
    "install-client":"cd client && npm install",
    "build-client": "cd client && npm run build",
    "server": "nodemon server --ignore client",
    "client": "npm start --prefix client",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \" npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhengcai94/coding_challenge.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zhengcai94/coding_challenge/issues"
  },
  "homepage": "https://github.com/zhengcai94/coding_challenge#readme",
  "devDependencies": {
    "concurrently": "^7.5.0",
    "nodemon": "^2.0.20"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.1",
    "morgan": "^1.10.0",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  }
}
